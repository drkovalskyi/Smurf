
The following extra packages need to be checked out
do this from CMSSW_x_y_z/src/

# get the lepton tree maker
# with a TAG that you should alredy know from
# http://www.t2.ucsd.edu/tastwiki/bin/view/Smurf/TPandFR2012
cvs co -rTAG -d Smurf/Core UserCode/Smurf/Core
cvs co -rTAG -d Smurf/ProcessingAndSkimming UserCode/Smurf/ProcessingAndSkimming

# egamma pf isolation config
# see 
# https://twiki.cern.ch/twiki/bin/view/CMS/EgammaPFBasedIsolation#Recommended_electron_vetoes
# hint - it depends on what release you are using so take care

cvs co HiggsAnalysis/HiggsToWW2Leptons/
cvs co -d Smurf/ProcessingAndSkimming/data/ElectronMVAWeights/ UserCode/MitPhysics/data/ElectronMVAWeights/
cvs co -d Smurf/ProcessingAndSkimming/data/MuonMVAWeights/ UserCode/MitPhysics/data/MuonMVAWeights/
cvs co -d EGamma/EGammaAnalysisTools UserCode/EGamma/EGammaAnalysisTools

cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat1.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat2.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat3.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat4.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat5.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_NonTrigV0_Cat6.weights.xml Smurf/ProcessingAndSkimming/data/

cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat1.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat2.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat3.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat4.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat5.weights.xml Smurf/ProcessingAndSkimming/data/
cp /afs/cern.ch/cms/data/CMSSW/RecoEgamma/ElectronIdentification/data/Electrons_BDTG_TrigV0_Cat6.weights.xml Smurf/ProcessingAndSkimming/data/

# 2012 HZZ ID+Iso combined MVA: does not include IP variables though.
cvs co -rV0-00-01 -d Muon/MuonAnalysisTools UserCode/sixie/Muon/MuonAnalysisTools

