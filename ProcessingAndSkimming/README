
The following extra packages need to be checked out
do this from CMSSW_x_y_z/src/

# get the lepton tree maker
# with a TAG that you should alredy know from
# http://www.t2.ucsd.edu/tastwiki/bin/view/Smurf/TPandFR2012
cvs co -rTAG -d Smurf/Core UserCode/Smurf/Core
cvs co -rTAG -d Smurf/ProcessingAndSkimming UserCode/Smurf/ProcessingAndSkimming

# egamma pf isolation config
# see 
# https://twiki.cern.ch/twiki/bin/view/CMS/EgammaPFBasedIsolation#Recommended_electron_vetoes
# hint - it depends on what release you are using so take care

# for Si Xie thesis MVA
# using combined ID+Isolation
cvs co -rHWWID_V00-00-00 HiggsAnalysis/HiggsToWW2Leptons/

# for EGamma MVA
cvs co -rV00-00-06 -dEGamma/EGammaAnalysisTools UserCode/EGamma/EGammaAnalysisTools
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat1.weights.xml
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat2.weights.xml
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat3.weights.xml
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat4.weights.xml
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat5.weights.xml
cp /afs/cern.ch/cms/higgs/higgsww/dmytro/selectors/summer12/Electrons_BDTG_TrigV0_Cat6.weights.xml

# 2012 HZZ ID+Iso combined MVA
cvs co -rR2012A_V1 -d Muon/MuonAnalysisTools /UserCode/sixie/Muon/MuonAnalysisTools

# 2012 muon radial isolation calculator
cvs co -rV00-00-01 -d MuonIsolation/Tools/ UserCode/folgueras/MuonIsolation/Tools/

